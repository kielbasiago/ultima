{
  "name": "@ff6wc/ultima",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "vercel": "vercel",
    "vercel-dev": "pnpm --filter \"@ff6wc/balance-and-ruin\" run  vercel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@ff6wc/eslint-config-custom": "workspace:*",
    "@types/node": "^17.0.12",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.14.2"
}
